<table class="table table-bordered">
  <thead>
    <tr>
      <th>Name</th>
      <th>Options</th>
    </tr>
  </thead>
  <tbody>
    <% tags.each do |tag|%>
      <%= render partial: "tags/row", locals: { tag: tag } %>
    <% end %>
  </tbody>
</table>

<%= render partial: "tags/edit", locals: { tag: tags.last } %>
